<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Pesti Vibe</title>
  </head>
  <body>
    <p>Number of events: {{events.length}}</p>
    <table border='1'>
      <tr>
        <th>event</th>
        <th>place</th>
        <th>at</th>
      </tr>
      {{#each events}}
        <tr>
          <td>{{this.name}}</td>
          <td>{{this.place.name}}</td>
          <td>{{this.startDateTimeFormatted}}</td>
        </tr>
      {{/each}}
    </table>
  </body>
</html>